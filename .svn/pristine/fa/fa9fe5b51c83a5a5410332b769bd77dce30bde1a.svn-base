import javax.swing.*;
import java.awt.*;
import java.awt.image.BufferedImage;
import java.util.ArrayList;

public class CollageTestPanel extends JPanel {
    private CollageGraphicsPainter collagePainter;

    public CollageTestPanel(Dimension dimensions, int numColumns, ArrayList<BufferedImage> imageList)
    {
        collagePainter = new CollageGraphicsPainter(dimensions, numColumns, imageList);
    }

    @Override
    public void paint(Graphics g)
    {
        collagePainter.paintCollage(g);
    }
}
