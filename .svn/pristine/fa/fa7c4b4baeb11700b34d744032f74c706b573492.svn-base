public class BankAccountRefactoredTestsMine {

    public static void main(String[] args) {
        BankAccountRefactoredTestsMine.test_creation();
        BankAccountRefactoredTestsMine.test_deposit();
        BankAccountRefactoredTestsMine.test_withdraw();
        BankAccountRefactoredTestsMine.test_credit_interest();
        System.out.println("Success!");
    }

    static void test_creation() {
        BankAccountRefactoredMine a = new BankAccountRefactoredMine(100.0f, 31, 0.0001f);
        assert a.balance == 100.0f : "Incorrect balance!";
        assert a.interestEarned == 0.0f : "Incorrect interest!";
        assert a.dayLastOp == 31 : "Incorrect day last operation!";
        assert a.interestRate == 0.0001f : "Incorrect interest rate!";
    }

    static void test_deposit(){
        BankAccountRefactoredMine a = new BankAccountRefactoredMine(100.0f,31,0.0001f);
        a.deposit(50f, 61);
        assert a.balance==150f;
        assert a.interestEarned>0.299999f && a.interestEarned<0.3000001f;
        assert a.dayLastOp == 61;
    }

    static void test_withdraw(){
        BankAccountRefactoredMine a = new BankAccountRefactoredMine(100.0f,31,0.0001f);
        a.withdraw(50f, 61);
        assert a.balance==50f;
        assert a.interestEarned>0.299999f && a.interestEarned<0.3000001f;
        assert a.dayLastOp == 61;
    }

    static void test_credit_interest() {
        BankAccountRefactoredMine a = new BankAccountRefactoredMine(100.0f,31,0.0001f);
        a.deposit(50f, 61);
        a.credit_interest();
        assert a.balance==150.3f;
        assert a.interestEarned==0.0f;
    }
}
