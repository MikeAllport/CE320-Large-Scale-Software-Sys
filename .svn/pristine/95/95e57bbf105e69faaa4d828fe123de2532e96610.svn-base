package group9.ButtonHandling;

import group9.CollageWindow.CollagePage;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

public class PreviousCollageButtonHandler implements ActionListener {
    private final CollagePage page;
    public PreviousCollageButtonHandler(CollagePage page) {
        this.page = page;
    }

    @Override
    public void actionPerformed(ActionEvent e) {
        page.setCollagePreviewImage(-1);
        System.out.println("CODE WILL CYCLE TO PREVIOUS COLLAGE");
    }
}
