import java.awt.image.BufferedImage;

public class CollageBufferedImage implements Comparable<CollageBufferedImage>{
    BufferedImage image;
    Bounds bounds;

    CollageBufferedImage(BufferedImage image, Bounds bounds)
    {
        this.image = image;
        this.bounds = bounds;
    }

    @Override
    public int compareTo(CollageBufferedImage o) {
        double thisSize = bounds.getSize().width + bounds.getSize().height;
        double otherSize = o.bounds.getSize().width + o.bounds.getSize().height;
        return (int)(otherSize - thisSize);
    }
}
